<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FacebookGUI.Controllers.SimulatePostController">
   <children>
      <Label fx:id="name" alignment="CENTER" contentDisplay="CENTER" layoutX="90.0" layoutY="7.0" prefHeight="32.0" prefWidth="123.0" text="Add New Post" textAlignment="CENTER">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="newPost" alignment="TOP_LEFT" layoutX="25.0" layoutY="46.0" prefHeight="209.0" prefWidth="250.0" />
      <Button fx:id="submitButton" layoutX="124.0" layoutY="261.0" mnemonicParsing="false" onAction="#submitPost" text="Submit" />
   </children>
</AnchorPane>
